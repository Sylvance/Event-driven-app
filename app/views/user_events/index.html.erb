<p id="notice"><%= notice %></p>

<h1>User Event Data</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Type</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_events.each do |user_event| %>
      <tr>
        <td><%= user_event.user_id %></td>
        <td><%= user_event.event_type %></td>
        <td><%= user_event.value %></td>
        <td><%= link_to 'Show', user_event %></td>
      </tr>
    <% end %>
  </tbody>
</table>
